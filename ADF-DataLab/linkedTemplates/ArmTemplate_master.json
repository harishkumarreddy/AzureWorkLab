{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-DataLab"
		},
		"LNK_SADatalab_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LNK_SADatalab'"
		},
		"LNK_DBS_DataLab_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LNK_DBS_DataLab'"
		},
		"LNK_Harish_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://HarishWorkLabSecrets.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-DataLab_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LNK_SADatalab_connectionString": {
						"value": "[parameters('LNK_SADatalab_connectionString')]"
					},
					"LNK_DBS_DataLab_connectionString": {
						"value": "[parameters('LNK_DBS_DataLab_connectionString')]"
					},
					"LNK_Harish_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LNK_Harish_KeyVault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}